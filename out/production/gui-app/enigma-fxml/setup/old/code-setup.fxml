<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<BorderPane fx:id="codeSetupBPane" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.ConfigurationController">
   <bottom>
      <HBox alignment="CENTER" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="randomSetupBtn" mnemonicParsing="false" text="Set Random Code" />
            <Button fx:id="machineSetupBtn" mnemonicParsing="false" text="Create Machine" />
         </children>
         <padding>
            <Insets bottom="20.0" />
         </padding>
      </HBox>
   </bottom>
   <center>
      <VBox fx:id="setupVbox" alignment="CENTER" prefHeight="200.0" prefWidth="250.0" BorderPane.alignment="CENTER">
         <children>
            <Label text="Nothing to show yet" />
         </children>
      </VBox>
   </center>
</BorderPane>
