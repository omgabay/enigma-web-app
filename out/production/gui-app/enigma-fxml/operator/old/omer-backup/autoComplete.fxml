<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.AutoCompleteController">
   <content>
      <VBox alignment="CENTER" spacing="10.0">
         <children>
            <Label alignment="TOP_CENTER" text="Auto Complete Suggestions" />
            <TextField fx:id="text" promptText="Input your text here" />
            <AnchorPane VBox.vgrow="ALWAYS">
               <children>
                  <ButtonBar>
                     <buttons>
                        <Button fx:id="autoComplete1" mnemonicParsing="false" onAction="#btnSelected" />
                        <Button fx:id="autoComplete2" alignment="CENTER" mnemonicParsing="false" onAction="#btnSelected" />
                        <Button fx:id="autoComplete3" alignment="CENTER" mnemonicParsing="false" onAction="#btnSelected" />
                        <Button fx:id="autoComplete4" alignment="CENTER" mnemonicParsing="false" onAction="#btnSelected" />
                        <Button fx:id="autoComplete5" alignment="CENTER" mnemonicParsing="false" onAction="#btnSelected" />
                     </buttons>
                  </ButtonBar>
               </children>
               <VBox.margin>
                  <Insets left="5.0" right="10.0" />
               </VBox.margin>
            </AnchorPane>
         </children>
         <padding>
            <Insets bottom="5.0" right="15.0" top="5.0" />
         </padding>
      </VBox>
   </content>
</ScrollPane>
